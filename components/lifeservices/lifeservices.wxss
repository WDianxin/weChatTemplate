/* components/typeWash/typeWash.wxss */
.lifeservicesBox {
  display: flex;
  flex-direction: column;
  justify-content: start;
  align-items: center;
  flex-wrap: wrap;
  width: 100%;
  overflow-y: scroll;
  height: 220rpx;
  box-sizing: border-box;
  padding-bottom: 20rpx;
}


.lifeservicesBox .li {
  width: 284rpx;
  height: 190rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  box-sizing: border-box;
  margin-left: 15rpx;
}


.lifeservicesBox .li image {
  display: inline-block;
  width: 284rpx;
  height: 190rpx;
  object-fit: cover;
  border-radius: 8rpx;
}

.lifeservicesBox .li text {
  color: #fff;
  font-size: 28rpx;
  margin-top: -40rpx;
  font-weight: bold;
  text-align: left;
  margin-left: 10rpx;
}
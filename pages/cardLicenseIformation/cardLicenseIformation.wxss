/* pages/cardLicenseIformation/cardLicenseIformation.wxss */
@import "../../template/registerSteps/registerSteps.wxss";
.submit-btn{
  width: 85%;
  margin-bottom: 30rpx;
}
/* 控制星号样式 */
.van-cell--required:before {
  left: 36rpx!important;
}
.van-cell__title , .van-cell__title {
 margin-left: 18rpx!important;
 font-weight: 600;
}
.idImgBox {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
  align-items: center;
  background: #fff;
  padding: 0 100rpx;
}
.yingYeBox{
  justify-content: center;
}
.menDian{
  padding: 0 20rpx;
}

.imgLeft,
.imgRegit {
  background: #F7F7F7;
  /* padding-top: 30rpx; */
  margin-top: 30rpx;
  /* width: 240rpx; */
}

.but {
  background-color: #6299E2;
  line-height: 52rpx;
  text-align: center;
  font-size: 24rpx;
  color: rgba(255, 255, 255, 100);
  border-radius: 0px 0px 10rpx 10rpx;
  width: 240rpx;
  margin-top: -8rpx;
}

.idImgBox .imgLeft image,
.idImgBox .imgRegit image {
  display: inline-block;
  width: 240rpx;
  height: 160rpx;
  object-fit: cover;
  /* margin-right: 10rpx; */
  border-radius: 6rpx 6rpx 0 0;
}
.menDian  .imgLeft image, .menDian  .imgRegit image{
  width: 180rpx;
  height: 180rpx;
}
.menDian .but{
  width: 180rpx;
  margin-top: 2rpx;
}

.remark {
  background: #fff;
  padding: 20rpx 40rpx;
  color: #999999;
  font-size: 24rpx;
  line-height: 40rpx;
}

.remark .ts {
  color: #6299E2;
}

.remark .zt {
  color: #FC6F80;
}
.but2{
  width: 100%;
  padding-bottom: 20rpx;
  background: #fff;
  text-align: center;
}
.but2 button{
  width: 180rpx;
  margin-left: 30rpx;
}

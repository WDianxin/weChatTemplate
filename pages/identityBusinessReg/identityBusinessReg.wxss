/* pages/identityBusinessReg/identityBusinessReg.wxss */
@import  "../../template/registerSteps/registerSteps.wxss";
.container{
  padding-bottom: 30rpx;
  box-sizing: border-box;
}
/* 控制星号样式 */
.van-cell--required:before {
  left: 36rpx !important;
}

.van-cell__title,
.van-cell__title {
  margin-left: 18rpx !important;
  font-weight: 600;
}

.but {
  background-color: #6299E2;
  line-height: 52rpx;
  text-align: center;
  font-size: 24rpx;
  color: rgba(255, 255, 255, 100);
  border-radius: 0px 0px 10rpx 10rpx;
  width: 240rpx;
  margin-top: -8rpx;
}
.idImgBox{
 background: #fff;
}
.imgLeft{
  background: #fff;
  width: 240rpx;
  margin: 0 auto;
  padding-top: 20rpx;
  /* padding-top: 30rpx; */
}
.idImgBox  image{
  display: inline-block;
  width: 240rpx;
  height: 160rpx;
  object-fit: cover;
  /* margin-right: 30rpx; */
  border-radius: 6rpx 6rpx 0 0;
}
.remark {
  background: #fff;
  padding: 20rpx 40rpx;
  color: #999999;
  font-size: 24rpx;
  line-height: 40rpx;
}

.remark .ts {
  color: #6299E2;
}

.remark .zt {
  color: #FC6F80;
}
.but2{
  width: 100%;
  padding-bottom: 20rpx;
  background: #fff;
  text-align: center;
}
.but2 button{
  width: 180rpx;
  margin-left: 30rpx;
}
.submit-wrapper {
  margin: 60rpx auto 0;
  width: 85%;
}